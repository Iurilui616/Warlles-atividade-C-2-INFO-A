#include <stdio.h>
#include <stdlib.h>
int main() {
    int raio;
    int altura;
    int volume;

    printf("Raio da base da lata (em cm): ");
    scanf("%d", &raio);

    printf("Altura da lata (em cm): ");
    scanf("%d", &altura);

    volume = 3.14 * raio * raio * altura;

    printf("O volume da lata e: %d cmÂ³\n", volume);

    return 0;
}
