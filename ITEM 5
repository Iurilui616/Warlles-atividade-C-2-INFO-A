#include <stdio.h>
#include <stdlib.h>
int main() {
    float raio;
    float altura;
    float volume;

    printf("Raio da base da lata (em cm): ");
    scanf("%f", &raio);

    printf("Altura da lata (em cm): ");
    scanf("%f", &altura);

    volume = 3.14 * raio * raio * altura;

    printf("O volume da lata e: %.2f cmÂ³\n", volume);

    return 0;
}
